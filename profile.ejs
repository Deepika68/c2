<!DOCTYPE html>
<html lang="en">
<head>
   <meta charset="UTF-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <title>Coffee Bean | My Account</title>

   <!-- font awesome cdn link  -->
   <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">

   <!-- custom css file link  -->
   <link rel="stylesheet" href="css/profile.css">

</head>
<body>
    <section class="user-details" form action="profile" method="post">

    <div class="user" >
     
      <img src="images/avatar.jpg" alt="">
      <%customersList.forEach( customer=> {%>
       <p><i class="fas fa-user"></i> <span><%= customer.username %></span></p>
       <p><i class="fas fa-envelope"></i> <span><%=customer.email %></span></p>
       <p><i class="fas fa-eye"></i> <span><%=customer.password %></span></p>
       <p><i class="fas fa-map-marker-alt""></i> <span><%=customer.cus_address %></span></p>
       <p><i class="fas fa-phone"></i> <span><%= customer.cus_ph %></span></p>
       <a href="/update_prof" class="btn">EDIT PROFILE</a>
       <a href="/logout" class="btn">logout</a>
       <%})%> 
    </div>
 
 </section>
 <script src="script.js"></script>
 </body>

 </html>